<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="TrabalhoPGBDPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>br.unisc.pickglobe.model.Site</class>
    <class>br.unisc.pickglobe.model.Link</class>
    <class>br.unisc.pickglobe.model.PalavraLink</class>
    <class>br.unisc.pickglobe.model.ListaPalavras</class>
    <class>br.unisc.pickglobe.model.TipoLista</class>
    <class>br.unisc.pickglobe.model.Palavra</class>
    <class>br.unisc.pickglobe.model.ListaExtensoes</class>
    <class>br.unisc.pickglobe.model.Extensao</class>
    <class>br.unisc.pickglobe.model.Coleta</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/pickglobe?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="pickglobe"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="pickglobe"/>
    </properties>
  </persistence-unit>
</persistence>
